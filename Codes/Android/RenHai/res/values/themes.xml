<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
      
    <style name="ActionBarWithOnlyTitleTheme" parent="@style/AppTheme">  
        <item name="android:actionBarStyle">@style/ActionBarWithOnlyTitle</item>     
    </style>  
  
    <!-- ActionBar styles -->  
    <style name="ActionBarWithOnlyTitle" parent="@android:style/Widget.Holo.ActionBar">  
        <item name="android:background">@color/maingreen</item>  
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>     
    </style>  
  
    <!-- ActionBar title text -->  
    <style name="MyActionBarTitleText" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">  
        <item name="android:textColor">@color/white</item>  
        <item name="android:textSize">@dimen/protocal_title_size</item>
    </style>  
   
</resources>
